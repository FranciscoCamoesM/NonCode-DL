#!/bin/bash

echo "Running training scripts for E23H3 with different hyperparameters..."

for W in 1e-4 5e-4  1e-3; do
    for L in 1e-4 1e-5 1e-6 1e-5; do
        python 1_train_net.py --datafolder Fasta_Pool3 --enh E22P3G3 --lr $L --wd $W --epochs 600 --seed 5
    done
done

echo "Training complete!"
